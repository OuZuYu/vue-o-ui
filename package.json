{
  "name": "vue-o-ui",
  "version": "0.1.0",
  "description": "基于 Vue 的 ui组件库",
  "main": "lib/oui.umd.min.js",
  "keyword": "o-ui oui",
  "private": false,
  "scripts": {
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lib": "vue-cli-service build --target lib --name oui --dest lib packages/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "vue": "^2.6.6",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "husky": "^3.0.9",
    "prettier": "1.18.2",
    "pretty-quick": "^2.0.0",
    "vue-styleguidist": "^3.25.0",
    "vue-template-compiler": "^2.5.21"
  }
}
